{
  "version": "1.0.4",
  "buildOptions": {
    "warningsAsErrors": true,
    "allowUnsafe": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "ASP.NET Core middleware for caching HTTP responses on the server.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/ResponseCaching"
    },
    "tags": [
      "aspnetcore",
      "cache",
      "caching"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Http": "1.0.4",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.4",
    "Microsoft.AspNetCore.ResponseCaching.Abstractions": "1.0.4",
    "Microsoft.Extensions.Caching.Memory": "1.0.2",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.2"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Configuration": ""
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.AppContext": "4.1.0"
      }
    }
  }
}